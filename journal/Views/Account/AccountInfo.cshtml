@model journal.ViewModels.UserViewModels
@{
    ViewBag.Title = "AccountInfo";
}

<h2>AccountInfo</h2>

<div>      
    <hr />
    <dl class="dl-horizontal">
        <dt>
            First Name
        </dt>

        <dd>
            @Model.FirstName
        </dd>

        <dt>
            Last Name
        </dt>

        <dd>
            @Model.LastName
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Model.Email
        </dd>
        <dt>
            Age
        </dt>
        <dd>
            @Model.Age
        </dd>
        <dt>
            School
        </dt>
        <dd>
            @Model.SelectedSchool
        </dd>
        <dt>
            Class
        </dt>
        <dd>
            @Model.ClassSelected
        </dd>
        <dt>
            Degree
        </dt>
        <dd>
            @Model.Degree
        </dd>
        <dt>
            Info
        </dt>
        <dd>
            @Model.Info
        </dd>
        <dt>
            Phone
        </dt>
        <dd>
            @Model.Phone
        </dd>
        <dt>
            Role
        </dt>
        <dd>
            @Model.UserRollSelected
        </dd>
        <dt>
            Data of created
        </dt>
        <dd>
            @Model.RegisterDate
        </dd>        
    </dl>
</div>
<p>
    <a href="~/Account/EditAccount/@Model.ID">Edit</a> 
</p>
